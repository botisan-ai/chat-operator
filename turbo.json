{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "dependsOn": [
        "^prebuild",
        "^build"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^chat-operator-backend#db:generate"
      ]
    },
    "clean": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "prebuild": {
      "cache": false
    }
  }
}