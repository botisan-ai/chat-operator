    "date-fns": "^2.28.0",
    "fast-xml-parser": "^4.0.0-beta.8",
    "form-data": "^4.0.0",
    "ioredis": "^4.28.2",
    "lru-cache": "^6.0.0",
    "mime-types": "^2.1.34",
    "minio": "^7.0.25",
