{
  "name": "chat-operator",
  "version": "0.0.3",
  "description": "",
  "author": "Simon Liang <simon@x-tech.io>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "patch-package",
    "prebuild": "yarn clean:nest && yarn clean:next && yarn db:generate",
    "build": "yarn build:nest && yarn build:next",
    "build:prod": "NODE_ENV=production yarn build",
    "build:nest": "nest build",
    "build:next": "next build",
    "db:generate": "prisma generate",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "clean:nest": "rimraf dist",
    "clean:next": "rimraf .next",
    "format": "prettier --write",
    "lint": "eslint --fix",
    "docgen": "compodoc -p tsconfig.json",
    "nuke": "docker-compose down -v && sh init_minio.sh && docker-compose up -d"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
